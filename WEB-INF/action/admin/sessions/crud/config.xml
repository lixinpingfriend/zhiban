<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
		活动的进程管理
	</summary>
	
	<log>false</log>

	<set-request-attribute id="help.msg" value="Online help for VIEW page..." />
	<set-request-attribute id="header.title" value="活动的进程管理" />
	<set-request-attribute id="header.url" value='123'/>

    <set-request-attribute id="formTitleAddNew" value='新增活动的进程'/>
    <set-request-attribute id="formTitleEdit" value='修改活动的进程'/>
    <set-request-attribute id="formTitleFilter" value='查询活动的进程'/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>		
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
	</output>

</config>
