<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
		人员资质管理
	</summary>
	
	<log>false</log>

	<set-request-attribute id="header.title" value="人员资质管理" />
    <set-request-attribute id="formTitleAddNew" value='新增人员资质'/>
    <set-request-attribute id="formTitleEdit" value='修改人员资质'/>
    <set-request-attribute id="formTitleFilter" value='查询人员资质'/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
	</output>

</config>
