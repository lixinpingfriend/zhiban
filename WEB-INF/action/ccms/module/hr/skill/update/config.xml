<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
		更新技能信息
	</summary>
	
	<log>false</log>

	<colname>role_id;role_id</colname>
	<sql-template>insert-skill-roles.sql;insert-user-roles.sql</sql-template>

	<transaction>
		<classname>com.ccms.InsertMaster</classname>
		<validator>true</validator>
		<transaction>true</transaction>
		<jdbc-log>false</jdbc-log>
	</transaction>

	<query>update_t_skill.sql</query>
	<query>delete-skill-roles.sql</query>
	<query>delete-user-roles.sql</query>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>script.js</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/javascript</content-type>
		<expiration>0</expiration>
	</output>

</config>
